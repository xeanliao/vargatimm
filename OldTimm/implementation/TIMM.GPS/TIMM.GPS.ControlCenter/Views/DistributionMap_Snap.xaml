<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:map="clr-namespace:Microsoft.Maps.MapControl;assembly=Microsoft.Maps.MapControl" 
	x:Class="TIMM.GPS.ControlCenter.Views.DistributionMap_Snap"
    mc:Ignorable="d"
    d:DesignHeight="600" d:DesignWidth="800">
    
    <Grid x:Name="LayoutRoot" Background="Black">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Button x:Name="btnStart" Click="btnStart_Click" Content="Start" Width="120" Height="30" VerticalAlignment="Center" HorizontalAlignment="Center" />
        
        <map:Map Name="map"
            IsEnabled="False"
            Mode="Aerial"
			Center="39.3683,-95.2734" ZoomLevel="12.0" 
			ScaleVisibility="Collapsed" 
			NavigationVisibility="Collapsed"
			LogoVisibility="Collapsed"
			CopyrightVisibility="Collapsed"/>
        <Button x:Name="btnMock" Click="btnMock_Click" Visibility="Collapsed" Content="Mock" Width="120" Height="30" VerticalAlignment="Top" HorizontalAlignment="Right" />
    </Grid>
</UserControl>
