<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="GPS.DomainLayer"  namespace="GPS.DomainLayer.Entities">
  <class name ="BlockGroupCoordinate" table="BlockGroupCoordinates">
    <id name="Id" column ="Id" unsaved-value="0">
      <generator class="GPS.DataLayer.SequenceGenerators.SequenceGenerator, GPS.DataLayer">
        <param name="table">SequenceCounters</param>
        <param name="column">BlockGroupCoordinate</param>
      </generator>
    </id>
    <property name ="Latitude" />
    <property name ="Longitude" />
    <many-to-one name="BlockGroup"
                 column="BlockGroupId"
                 not-null="true"
                 class="GPS.DomainLayer.Entities.BlockGroup, GPS.DomainLayer" />
  </class>
</hibernate-mapping>
