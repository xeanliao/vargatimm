<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit https://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>
		<add key="TilesCachePath" value="C:\\inetpub\\wwwroot\\cache" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
		<add key="TempPath" value="~/Files/Temp/" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
		<add key="Passphrase" value="www.timm.com" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
	</appSettings>
	<connectionStrings>
		<add name="TIMMContext"
		  connectionString="server=172.31.24.114; database=TimmPreview; user id=sa; password=harry4h#; timeout=300"
		  xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
	</connectionStrings>
	<log4net>
		<appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender" xdt:Transform="Replace" xdt:Locator="Match(name)">
			<file value="Logs\\" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<datePattern value="'website'.yyyy-MM-dd.'log'" />
			<staticLogFileName value="false" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger %newline%message%newline" />
			</layout>
		</appender>
	</log4net>
</configuration>